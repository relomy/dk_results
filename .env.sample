# Copy to .env and fill in values. Loaded by dotenv in
# find_new_double_ups.py and classes/cookieservice.py.

# SQLite database path (update_contests.py:DB_FILE, bot/discord_bot.py:DB_PATH).
CONTESTS_DB_PATH=contests.db

# Toggle Discord notifications for update_contests.py.
DISCORD_NOTIFICATIONS_ENABLED=true

# Discord bot credentials (update_contests.py:_build_discord_sender,
# bot/discord_bot.py:BOT_TOKEN, bot/discord_bot.py:ALLOWED_CHANNEL_ID).
DISCORD_BOT_TOKEN=replace-me
DISCORD_CHANNEL_ID=123456789012345678

# Discord webhook for find_new_double_ups.py.
DISCORD_WEBHOOK=replace-me

# Google Sheets ID (classes/dfssheet.py:Sheet.__init__,
# update_contests.py:SPREADSHEET_ID, bot/discord_bot.py:SPREADSHEET_ID,
# generate_sheet_gids.py:main).
SPREADSHEET_ID=replace-me

# Sheet gid mapping file (update_contests.py:SHEET_GIDS_FILE,
# bot/discord_bot.py:SHEET_GIDS_FILE).
SHEET_GIDS_FILE=sheet_gids.yaml

# Warning schedule configuration (update_contests.py:WARNING_SCHEDULE_FILE_ENV,
# update_contests.py:CONTEST_WARNING_MINUTES).
CONTEST_WARNING_SCHEDULE_FILE=contest_warning_schedules.yaml
CONTEST_WARNING_MINUTES=25

# Optional bot log file (bot/discord_bot.py:DISCORD_LOG_FILE).
DISCORD_LOG_FILE=logs/discord_bot.log

# Cookie sourcing for DraftKings sessions (classes/cookieservice.py:get_rookie_cookies).
DK_PLATFORM=pi
COOKIES_DB_PATH=/path/to/chromium/Cookies
